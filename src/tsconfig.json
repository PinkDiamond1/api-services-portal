{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "@/logger": ["logger"],
            "@/services/*": ["servicests/*"],
        },
    },
    "include": [
        "**/*.ts"
    ],
  "exclude": [
    "node_modules",
    "nextapp",
    "dist"
  ]
}
